{
	"compilerOptions": {
		"skipLibCheck": true,
		"strict": true,
		"esModuleInterop": true,
		"module": "esnext",
		"moduleResolution": "Bundler",
		"resolveJsonModule": true,
		"isolatedModules": true,
		"incremental": false,
		"paths": {
			"@/*": ["./src/*"]
		}
	},
	"exclude": ["node_modules"]
}
